#

npx sequelize
sequelize init

#criar banco de dados
sequelize migration:generate --name criate_users

#roda todas as migrations
npx sequelize db:migrate

#gera um novo seedars
npx sequelize-cli seed:generate --name demo-user


#teste
yarn add jest -D 
yarn add supertests -D

yarn jest --init

yarn test
yarn test nomearquivo.js